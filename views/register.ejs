<%- include('partials/header') %>

<div class="container">

   

    <h1 class="d-3 text-center mb-3">Register with Us!!</h1>
    <div class="row justify-content-center m-5 ">
       
        <div class="col-lg-8 ">

            <form action="/register" method="POST">
                <div class="form-group">
                  <label for="name">UserName :</label>
                  <input type="text" class="form-control" name="username" aria-describedby="name" placeholder=" username">
                </div>
                <div class="form-group">
                  <label >Password:</label>
                  <input type="password" class="form-control" name="password" placeholder=" password">
                </div>
               
                <button type="submit" class="btn btn-primary btn-block mb-2">Sign up</button>
              </form>
              
             
        </div>
    </div>
    

</div>




<%- include("partials/footer") %>